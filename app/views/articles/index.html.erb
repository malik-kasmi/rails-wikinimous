
<h1>*Articles*</h1>
<ol>
<% @articles.each do |article| %>
  <li>
    <h2><%= link_to article.title, article_path(article) %></h2>
    <!-- <p><%= article.content %></p> -->
    <%= link_to "Delete", article_path(article),
        method: :delete,
        data: { confirm: "Are you sure?" } %>
    <%= link_to "Edit", edit_article_path(article) %>
  </li>
<% end %>
</ol>
<%= link_to "New", new_article_path %>
